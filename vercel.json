{
  "version" : 2,
"builds" : [{
      "src" : "./index.js",
      "use" : "@vercel/node"
  }
  ],
  "routes" : [
    {
      "src" : "/",
      "dest" :"/server.js"
    },
    {
      "src" : "/api/auth/(.*)",
      "dest" :"/routes/auth.js"
    },
        {
      "src" : "/api/(.*)",
      "dest" :"/routes/general.js"
    }
    
  ]
}
